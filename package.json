	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"deploy": "svelte-kit sync && svelte-package",
		"publish:npm": "cd package && npm publish",
		"publish": "run-s format deploy publish:npm",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@playwright/test": "^1.27.1",
		"@sveltejs/adapter-auto": "1.0.0-next.87",
		"@sveltejs/kit": "1.0.0-next.535",
		"@sveltejs/package": "1.0.0-next.5",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.7.1",
		"prettier-plugin-svelte": "^2.8.0",
		"svelte": "^3.44.0",
		"svelte-check": "^2.9.2",
		"svelte-preprocess": "^4.10.7",
		"typescript": "^4.8.4",
		"vite": "^3.2.7"
	},
	"dependencies": {
		"sass": "^1.56.0",
		"svelte": "^3.52.0",
		"svelte-petit-utils": "^0.3.5",
		"svelte-pointer-tracker": "^0.2.6",
